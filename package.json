{
  "name": "workspace-root",
  "version": "6.2.0",
  "private": true,
  "description": "Workspace root for ember-intl",
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-intl/ember-intl.git"
  },
  "license": "MIT",
  "author": "jason.mitchell.w@gmail.com",
  "type": "module",
  "scripts": {
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "release:changelog": "changeset version",
    "release:package": "pnpm --recursive publish",
    "start": "pnpm --filter docs-app start",
    "test": "pnpm --filter '*' test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@changesets/get-github-info": "^0.5.2"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "pnpm": {
    "overrides": {
      "@babel/core": "^7.23.3",
      "@babel/traverse": "^7.23.3",
      "fast-glob": "^3.3.1"
    }
  }
}
