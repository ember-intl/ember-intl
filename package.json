{
  "name": "ember-intl",
  "version": "6.0.0-beta.3",
  "description": "A internationalization toolbox for ambitious applications.",
  "keywords": [
    "ember-addon",
    "i18n",
    "ember-intl",
    "ember-i18n",
    "ember",
    "internationalization"
  ],
  "author": "jason.mitchell.w@gmail.com",
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://ember-intl.github.io/ember-intl/",
  "repository": {
    "type": "git",
    "url": "http://github.com/ember-intl/ember-intl.git"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:js": "eslint . --cache --ext=.js,.ts",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "start": "ember serve",
    "test": "npm-run-all --aggregate-output --continue-on-error --parallel test:ember test:node",
    "test:ember": "ember test",
    "test:ember-compatibility": "./node_modules/.bin/ember try:one",
    "test:node": "mocha \"tests-node/**/*.js\""
  },
  "dependencies": {
    "@formatjs/icu-messageformat-parser": "^2.0.18",
    "@formatjs/intl": "^1.6.7",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-files": "^0.8.0",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-source": "^3.0.0",
    "broccoli-stew": "^3.0.0",
    "calculate-cache-key-for-tree": "^2.0.0",
    "cldr-core": "38",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-typescript": "^4.0.0",
    "eventemitter3": "^4.0.7",
    "extend": "^3.0.2",
    "fast-memoize": "^2.5.2",
    "has-unicode": "^2.0.1",
    "intl-messageformat": "^9.4.3",
    "js-yaml": "4",
    "json-stable-stringify": "^1.0.1",
    "locale-emoji": "^0.3.0",
    "lodash.castarray": "^4.4.0",
    "lodash.last": "^3.0.0",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^1.0.4",
    "silent-error": "^1.1.1"
  },
  "peerDependencies": {
    "typescript": "4"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@formatjs/intl-pluralrules": "^4.0.6",
    "@formatjs/intl-relativetimeformat": "^8.0.4",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "@types/ember": "^3.16.3",
    "@types/ember-qunit": "^5.0.0",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__test-helpers": "^2.6.1",
    "@types/qunit": "^2.19.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "~3.24.0",
    "ember-cli-addon-docs": "^1.0.0",
    "ember-cli-addon-docs-esdoc": "^0.2.3",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-compatibility-helpers": "^1.2.2",
    "ember-data": "^3.25.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-open-browser": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.2",
    "ember-root-url": "^0.2.0",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^10.5.4",
    "loader.js": "^4.7.0",
    "mocha": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "testdouble": "^3.16.1",
    "testdouble-qunit": "^3.0.0",
    "typescript": "^4.0.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 14.*"
  },
  "typesVersions": {
    "^3.0": {
      "test-support": [
        "test-support/index.d.ts"
      ],
      "test-support/*": [
        "test-support/*"
      ],
      "*": [
        "types.ts3/*",
        "addon/*"
      ]
    }
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js,*.ts": [
      "eslint"
    ]
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://ember-intl.github.io/ember-intl/"
  }
}
