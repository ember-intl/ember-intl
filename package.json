    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.1.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-ember": "^6.1.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "loader.js": "^4.7.0",
