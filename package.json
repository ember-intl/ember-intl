{
  "name": "ember-intl",
  "version": "3.0.0",
  "description": "A internationalization toolbox for ambitious applications.",
  "keywords": [
    "ember-addon",
    "i18n",
    "ember-intl",
    "ember-i18n",
    "ember",
    "internationalization"
  ],
  "author": "jason.mitchell.w@gmail.com",
  "license": "BSD-3-Clause",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://github.com/ember-intl/ember-intl",
  "repository": {
    "type": "git",
    "url": "http://github.com/ember-intl/ember-intl.git"
  },
  "scripts": {
    "start": "ember serve",
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "tests-node": "mocha tests-node/**/*.*",
    "test": "npm run tests-node && ember test",
    "testall": "npm run tests-node && ember try:each",
    "prettier": "prettier --single-quote --print-width 120 --write \"{blueprints,config,lib,app,addon,addon-test-support,tests,tests-node}/**/*.js\""
  },
  "dependencies": {
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-cldr-data": "^1.1.2",
    "broccoli-funnel": "^1.2.0",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-source": "^1.1.0",
    "broccoli-stew": "^1.5.0",
    "chalk": "^1.1.3",
    "cldr-core": "28.0.0",
    "ember-cli-babel": "^6.11.0",
    "ember-intl-format-cache": "^2.6.0",
    "ember-intl-messageformat": "^3.2.0",
    "ember-intl-relativeformat": "^3.3.0",
    "extend": "^3.0.1",
    "has-unicode": "^2.0.1",
    "intl": "^1.2.5",
    "intl-messageformat-parser": "^1.3.0",
    "js-yaml": "^3.9.0",
    "json-stable-stringify": "^1.0.1",
    "locale-emoji": "^0.3.0",
    "mkdirp": "^0.5.1",
    "silent-error": "^1.1.0",
    "walk-sync": "^0.3.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "ember-cli": "^2.18.1",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-cli-update": "^0.21.3",
    "ember-code-snippet": "^1.9.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-native-dom-event-dispatcher": "^0.6.4",
    "ember-open-browser": "^0.5.1",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.18.0",
    "eslint": "^4.19.1",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.1.2",
    "fs-extra": "^3.0.1",
    "loader.js": "^4.2.3",
    "mocha": "^3.4.2",
    "prettier": "^1.13.0",
    "qunit-dom": "^0.4.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://ember-intl.github.io/"
  }
}
