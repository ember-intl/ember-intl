{
  "name": "ember-intl",
  "version": "5.5.0-beta.2",
  "description": "A internationalization toolbox for ambitious applications.",
  "keywords": [
    "ember-addon",
    "i18n",
    "ember-intl",
    "ember-i18n",
    "ember",
    "internationalization"
  ],
  "author": "jason.mitchell.w@gmail.com",
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://ember-intl.github.io/ember-intl/",
  "repository": {
    "type": "git",
    "url": "http://github.com/ember-intl/ember-intl.git"
  },
  "scripts": {
    "start": "ember serve",
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:js": "eslint --ext ts,js .",
    "lint:types": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "tests-node": "mocha \"tests-node/**/*.js\"",
    "test": "yarn run tests-node && ember test",
    "test-node": "yarn tests-node",
    "testall": "yarn run tests-node && ember try:each",
    "prettier": "prettier --single-quote --print-width 120 --write \"{blueprints,config,lib,app,addon,addon-test-support,tests,tests-node}/**/*.js\"",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-files": "^0.8.0",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-source": "^3.0.0",
    "broccoli-stew": "^3.0.0",
    "calculate-cache-key-for-tree": "^2.0.0",
    "cldr-core": "^36.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-typescript": "^4.0.0-rc.1",
    "extend": "^3.0.2",
    "fast-memoize": "^2.5.2",
    "has-unicode": "^2.0.1",
    "intl-messageformat": "^9.2.5",
    "intl-messageformat-parser": "^5.4.2",
    "js-yaml": "^3.13.1",
    "json-stable-stringify": "^1.0.1",
    "locale-emoji": "^0.3.0",
    "lodash.castarray": "^4.4.0",
    "lodash.last": "^3.0.0",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^1.0.4",
    "silent-error": "^1.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@formatjs/intl-pluralrules": "^3.4.0",
    "@formatjs/intl-relativetimeformat": "^7.2.0",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^3.4.9",
    "@types/ember-resolver": "^5.0.9",
    "@types/ember__test-helpers": "^1.7.2",
    "@types/qunit": "^2.9.1",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "~3.20.0",
    "ember-cli-addon-docs": "^0.7.0",
    "ember-cli-addon-docs-esdoc": "^0.2.3",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^5.0.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-testdouble-qunit": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-compatibility-helpers": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-open-browser": "^1.0.0",
    "ember-resolver": "^8.0.0",
    "ember-root-url": "^0.2.0",
    "ember-source": "~3.20.3",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^7.11.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "lint-staged": "^10.2.2",
    "loader.js": "^4.7.0",
    "mocha": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "qunit-dom": "^1.2.0",
    "testdouble": "^3.13.1",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js,*.ts": [
      "eslint"
    ]
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://ember-intl.github.io/ember-intl/"
  }
}
