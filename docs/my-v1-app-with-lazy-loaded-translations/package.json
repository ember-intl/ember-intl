{
  "name": "my-v1-app-with-lazy-loaded-translations",
  "version": "1.4.2",
  "private": true,
  "description": "A Webpack app to test publicOnly",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-intl/ember-intl.git"
  },
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "_build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:intl": "ember-intl-lint",
    "lint:intl:fix": "ember-intl-lint --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "start": "ember serve",
    "test": "ember test --test-port=0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@ember-intl/lint": "workspace:*",
    "@ember-intl/v1-compat": "workspace:*",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/broccoli-side-watch": "^1.1.0",
    "@embroider/compat": "^3.9.3",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^3.5.9",
    "@embroider/router": "^2.1.12",
    "@embroider/webpack": "^4.1.2",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.1.1",
    "@glint/template": "^1.7.4",
    "@shared-configs/ember-template-lint": "workspace:*",
    "@shared-configs/eslint-config-ember": "workspace:*",
    "@shared-configs/eslint-config-yaml": "workspace:*",
    "@shared-configs/prettier": "workspace:*",
    "@shared-configs/testem": "workspace:*",
    "@shared-configs/typescript": "workspace:*",
    "@types/qunit": "^2.19.13",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.10.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.3.1",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-intl": "workspace:*",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.10.1",
    "ember-template-imports": "^4.4.0",
    "ember-template-lint": "^7.9.3",
    "ember-test-selectors": "^7.1.0",
    "eslint": "^9.39.2",
    "loader.js": "^4.7.0",
    "my-v1-addon": "workspace:*",
    "my-v2-addon": "workspace:*",
    "prettier": "^3.8.1",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "typescript": "^5.9.3",
    "webpack": "^5.105.0"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  }
