{
  "name": "@ember-intl/v1-compat",
  "version": "0.1.0",
  "description": "Loads translations to support ember-intl as v2 addon",
  "keywords": [
    "ember",
    "ember-addon",
    "ember-intl",
    "i18n",
    "internationalization"
  ],
  "homepage": "https://ember-intl.github.io/ember-intl/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-intl/ember-intl.git"
  },
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test --test-port=0"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@formatjs/icu-messageformat-parser": "^2.11.4",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-source": "^3.0.1",
    "calculate-cache-key-for-tree": "^2.0.0",
    "cldr-core": "^47.0.0",
    "ember-auto-import": "^2.11.1",
    "ember-cli-babel": "^8.2.0",
    "extend": "^3.0.2",
    "intl-messageformat": "^10.7.18",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.3.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.3.0",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@shared-configs/ember-template-lint": "workspace:*",
    "@shared-configs/eslint-config-ember": "workspace:*",
    "@shared-configs/prettier": "workspace:*",
    "@shared-configs/testem": "workspace:*",
    "concurrently": "^9.2.1",
    "ember-cli": "~6.8.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-load-initializers": "^3.0.1",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.8.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.6.2",
    "qunit": "^2.24.2",
    "qunit-dom": "^3.5.0",
    "webpack": "^5.102.1"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://ember-intl.github.io/ember-intl/"
  }
}
