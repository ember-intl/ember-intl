{
  "name": "docs-app",
  "version": "6.2.2",
  "private": true,
  "description": "Docs app for ember-intl",
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-intl/ember-intl.git"
  },
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"app/**/*.css\" --cache",
    "lint:css:fix": "stylelint \"app/**/*.css\" --fix",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "release:documentation": "ember deploy production",
    "start": "ember serve",
    "test": "ember test --test-port=0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.1",
    "@embroider/compat": "^3.4.0",
    "@embroider/core": "^3.4.2",
    "@embroider/webpack": "^3.2.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.2.1",
    "@glint/environment-ember-loose": "^1.2.1",
    "@glint/environment-ember-template-imports": "^1.2.1",
    "@glint/template": "^1.2.1",
    "@shared-configs/ember-template-lint": "workspace:*",
    "@shared-configs/eslint-config-ember": "workspace:*",
    "@shared-configs/prettier": "workspace:*",
    "@shared-configs/stylelint": "workspace:*",
    "@shared-configs/testem": "workspace:*",
    "@shared-configs/typescript": "workspace:*",
    "@types/qunit": "^2.19.9",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.7.0",
    "ember-cli": "~5.4.1",
    "ember-cli-addon-docs": "^6.0.2",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~5.3.0",
    "ember-intl": "workspace:*",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.1.0",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-template-lint": "^5.13.0",
    "ember-test-selectors": "^6.0.0",
    "eslint": "^8.54.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.1.0",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "stylelint": "^15.11.0",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "homepage": "https://ember-intl.github.io/ember-intl"
}
